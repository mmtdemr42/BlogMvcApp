@model IEnumerable<BlogMvcApp.Models.Category>



<div class="well">
    <h4>Kategoriler</h4>
    <div class="row">
        @foreach (var category in Model)
        {
            <div class="col-lg-6">
                <ul class="list-unstyled">
                    <li>
                        <a href="/Blog/List/@category.Id">@category.CategoryName</a>
                    </li>
                </ul>
            </div>
        }
    </div>
</div>